! URxvt-specific stuff
URxvt*borderColor: #151515

URxvt*colorBD: #ffffff
URxvt*colorIT: #a761c2
URxvt*colorUL: #6d9cbe
URxvt*underlineColor: #333333

URxvt*highlightColor: #a761c2
URxvt*highlightTextColor: #151515

URxvt*cursorColor: #ff8939
URxvt*cursorColor2: #151515



URxvt.letterSpace: -1
URxvt*font: xft:Envy Code R for Powerline:regular:size=9
URxvt*imFont: xft:Envy Code R for Powerline:regular:size=9
URxvt*boldFont: xft:Envy Code R for Powerline:regular:size=9
URxvt*italicFont: xft:Envy Code R for Powerline:regular:size=9
URxvt*boldItalicFont: xft:Envy Code R for Powerline:regular:size=9
!
!URxvt*font: xft:Menlo for Powerline:regular:size=8
!URxvt*font: xft:Pragmata TT:regular:size=9
!URxvt*imFont: xft:Menlo for Powerline:regular:size=8
!URxvt*boldFont: xft:Menlo for Powerline:regular:size=8
!URxvt*italicFont: xft:Menlo for Powerline:regular:size=8
!URxvt*boldItalicFont: xft:Menlo for Powerline:regular:size=8
!URxvt*font: xft:Fixed:pixelsize=9
!URxvt*boldFont: xft:Fixed:pixelsize=9

URxvt*buffered: true

URxvt*cursorBlink: true
URxvt*cursorUnderline: false

URxvt*geometry: 75x15

!URxvt*reverseVideo: true

URxvt*depth: 24

URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: plain
URxvt*scrollColor: #000000
URxvt*scrollWithBuffer: false
URxvt*scrollTtyOutput: true

URxvt*urgentOnBell: true
!URxvt*visualBell: true

URxvt*pointerBlank: true
/* vim: set ts=2 sw=2 tw=0 :*/
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher:jumanji
#URxvt.urlLauncher:chromium
URxvt.matcher.button: 1

URxvt.tabbed.tabbar-fg: 0
URxvt.tabbed.tabbar-bg: 0 
URxvt.tabbed.tab-bg: 0 
URxvt.tabbed.tab-fg: 0 

URxvt.transparent: true
URxvt.shading: 10
