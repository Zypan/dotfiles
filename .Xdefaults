URxvt*foreground: #e8e8e8
URxvt*background: #050505

!black
*color0: #393939
*color8: #121212

!red
*color1: #da4939
*color9: #ff6c5c

!green
*color10: #509F7E
*color2: #61C29A

!yellow
*color3: #DB8D4D
*color11: #bc9458

!blue
*color4: #6d9cbe
*color12: #91C1E3

!magenta
*color5: #5e468c
*color13: #7f62b3

!cyan
*color6: #435d75
*color14: #6e98a4

!white
*color7: #dedede
*color15: #b0b0b0

! URxvt-specific stuff
URxvt*borderColor: #151515

URxvt*colorBD: #ffffff
URxvt*colorIT: #a761c2
URxvt*colorUL: #6d9cbe
URxvt*underlineColor: #333333

URxvt*highlightColor: #a761c2
URxvt*highlightTextColor: #151515

URxvt*cursorColor: #ff8939
URxvt*cursorColor2: #151515



!URxvt.letterSpace: -1
*font: *-*-*-*-*-*-10-*-*-*-*-*-*-*
!URxvt*font: xft:Montecarlo:regular:size=9
!URxvt*imFont: xft:Montecarlo:regular:size=9
!URxvt*boldFont: xft:Montecarlo:regular:size=9
!URxvt*italicFont: xft:Montecarlo:regular:size=9
!URxvt*boldItalicFont: xft:Montecarlo:regular:size=9

URxvt*font: xft:ProFont:regular:size=8
URxvt*imFont: xft:ProFont:regular:size=8
URxvt*boldFont: xft:ProFont:regular:size=8
URxvt*italicFont: xft:ProFont:regular:size=8
URxvt*boldItalicFont: xft:ProFont:regular:size=8
!
!URxvt*font: xft:Envy Code R for Powerline:regular:size=8
!URxvt*imFont: xft:Envy Code R for Powerline:regular:size=8
!URxvt*boldFont: xft:Envy Code R for Powerline:regular:size=8
!URxvt*italicFont: xft:Envy Code R for Powerline:regular:size=8
!URxvt*boldItalicFont: xft:Envy Code R for Powerline:regular:size=8

dzen2*font: *-*-*-*-*-*-4-*-*-*-*-*-*-*
!URxvt*font: xft:Pragmata TT:regular:size=8
!URxvt*imFont: xft:Pragmata TT:regular:size=8
!URxvt*boldFont: xft:Pragmata TT:regular:size=8
!URxvt*italicFont: xft:Pragmata TT:regular:size=8
!URxvt*boldItalicFont: xft:Pragmata TT:regular:size=8

URxvt*buffered: true

URxvt*cursorBlink: true
URxvt*cursorUnderline: false

!URxvt*geometry: 75x15

!URxvt*reverseVideo: true

URxvt*depth: 24

URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: plain
URxvt*scrollColor: #000000
URxvt*scrollWithBuffer: false
URxvt*scrollTtyOutput: true

URxvt*urgentOnBell: true
!URxvt*visualBell: true

URxvt*pointerBlank: true
/* vim: set ts=2 sw=2 tw=0 :*/
URxvt.perl-ext-common: default,matcher,url-select
URxvt.keysym.C-u: perl:url-select:select_next
URxvt.urlLauncher:dwb
URxvt.underlineURLs: true
#URxvt.urlLauncher:chromium
URxvt.matcher.button: 1

URxvt.tabbed.tabbar-fg: 0
URxvt.tabbed.tabbar-bg: 0 
URxvt.tabbed.tab-bg: 0 
URxvt.tabbed.tab-fg: 0 

URxvt.transparent: true
URxvt.shading: 5

!URxvt*meta8:True
!
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hitstyle:   hintslight
