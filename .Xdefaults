*foreground: #DDDDDD
*background: #151515

!black
*color0: #393939
*color8: #474747

!red
*color1: #da4939
*color9: #ff6c5c

!green
!*color2: #519f50
*color10: #a5c261

*color2: #505050
!*color10: #ffffff
!yellow
*color3: #cc7833
*color11: #bc9458

!blue
*color4: #6d9cbe
